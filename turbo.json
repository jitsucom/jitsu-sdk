{
  "pipeline": {
    "test": {
      "dependsOn": [
        "build",
        "^test"
      ]
    },
    "clean": {
      "dependsOn": [
        "^clean"
      ]
    },
    "build": {
      "dependsOn": [
        "clean",
        "^build"
      ]
    },
    "dev": {
      "cache": false
    }
  }
}
